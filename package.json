{
    "name": "thatonespot",
    "version": "1.0.0",
    "description": "My personal website",
    "main": "server.js",
    "devDependencies": {
        "@types/aws-sdk": "^2.7.0",
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.1",
        "@types/express-fileupload": "^1.1.0",
        "@types/express-session": "^1.15.12",
        "@types/ip": "^1.1.0",
        "@types/mongoose": "^5.7.3",
        "@types/node": "^11.10.4",
        "@types/nodemailer": "^6.2.0",
        "@types/uuid": "^3.4.4",
        "concurrently": "^5.1.0",
        "eslint": "^6.8.0",
        "nodemon": "^2.0.2",
        "source-map-support": "^0.5.12",
        "ts-nameof": "^5.0.0",
        "ts-node": "^8.10.1",
        "ttypescript": "^1.5.10"
    },
    "dependencies": {
        "@okta/oidc-middleware": "^2.0.0",
        "@types/ts-nameof": "^4.2.1",
        "aws-sdk": "^2.643.0",
        "dotenv": "^6.2.0",
        "esm": "^3.2.9",
        "express": "^4.17.3",
        "express-fileupload": "^1.1.6",
        "express-session": "^1.15.6",
        "ip": "^1.1.5",
        "mongoose": "^5.9.3",
        "nodemailer": "^6.2.1",
        "reflect-metadata": "^0.1.13",
        "typescript": "^3.8.3",
        "uuid": "^3.3.2",
        "vash": "^0.12.6"
    },
    "scripts": {
        "setup": "./scripts/dotenv.sh -f && npm i",
        "build": "ttsc",
        "start": "node -r esm -r source-map-support/register -r dotenv/config --unhandled-rejections=strict src/server.js",
        "dev": "ttsc && concurrently \"ttsc -w\" \"nodemon\""
    },
    "repository": {
        "type": "git"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "js"
    },
